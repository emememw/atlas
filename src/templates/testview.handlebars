<table id="table-cars" class="table table-striped table-hover">
	<thead>
		<tr>
			<th>Type</th>
			<th>Name</th>
			<th>Price</th>
		</tr>
	</thead>
	<tbody>	
		{{#.}}
			<tr>
				<td>Car</td>
				<td>{{name}}</td>
				<td>{{price}}</td>
			</tr>
		{{/.}}
	</tbody>
</div>

<script type="text/javascript">
	$(function() {
		$("#table-cars").dataTable();
	});
</script>
